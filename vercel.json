{
    "version": 2,
    "builds": [
        {
            "src": "landing/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        },
        {
            "src": "app/next.config.ts",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/app/(.*)",
            "dest": "/app/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/landing/$1"
        },
        {
            "src": "/app",
            "status": 308,
            "headers": {
                "Location": "/app/"
            }
        }
    ]
}
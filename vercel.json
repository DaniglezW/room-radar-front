{
    "builds": [
        {
            "src": "landing/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        },
        {
            "src": "app/package.json",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/app(/.*)?",
            "dest": "/app/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/landing/$1"
        }
    ]
}